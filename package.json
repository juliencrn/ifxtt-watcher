{
  "name": "@ifxtt/watcher",
  "version": "1.0.0",
  "private": true,
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist",
    "tsc": "tsc -p ./tsconfig.json",
    "build": "yarn clean && yarn tsc",
    "dev": "yarn tsc --watch",
    "start": "yarn build && node build/index.js",
    "serve": "node dist/index.js",
    "lint": "eslint \"./src/**/*.ts*\" --fix",
    "test": "yarn tsc --noEmit && yarn lint"
  },
  "dependencies": {
    "axios": "^1.1.3",
    "dotenv": "^16.0.3"
  },
  "engines": {
    "node": "16"
  },
  "devDependencies": {
    "@types/node": "^18.11.7",
    "eslint": "^8.26.0",
    "rimraf": "^3.0.2",
    "typescript": "^4.8.4"
  }
}